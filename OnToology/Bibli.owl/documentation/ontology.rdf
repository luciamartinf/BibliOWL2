        <rdfs:label xml:lang="en">Cultural Institute or Site</rdfs:label>
        <rdfs:label xml:lang="it">Istituto e Luogo Della Cultura</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:Class>
    


